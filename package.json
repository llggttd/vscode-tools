{
  "name": "power-tool",
  "displayName": "PowerTool",
  "description": "a collection of power tools.",
  "publisher": "0z0ne",
  "icon": "icon.png",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/llggttd/vscode-tools.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:json.format",
    "onCommand:json.minify",
    "onCommand:date.currentDate",
    "onCommand:date.currentTime",
    "onCommand:url.encodeURI",
    "onCommand:url.decodeURI",
    "onCommand:url.encodeURIComponents",
    "onCommand:url.decodeURIComponents",
    "onCommand:util.uniqueId",
    "onCommand:util.base64Encode",
    "onCommand:util.base64Decode",
    "onCommand:url.escape",
    "onCommand:url.unescape"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json.format",
        "title": "%tools.format.json%"
      },
      {
        "command": "json.minify",
        "title": "%tools.minify.json%"
      },
      {
        "command": "date.currentDate",
        "title": "%tools.current.date%"
      },
      {
        "command": "date.currentTime",
        "title": "%tools.current.time%"
      },
      {
        "command": "url.encodeURI",
        "title": "%tools.url.encode%"
      },
      {
        "command": "url.decodeURI",
        "title": "%tools.url.decode%"
      },
      {
        "command": "url.encodeURIComponents",
        "title": "%tools.url.components.encode%"
      },
      {
        "command": "url.decodeURIComponents",
        "title": "%tools.url.components.decode%"
      },
      {
        "command": "util.uniqueId",
        "title": "%tools.util.uniqueId%"
      },
      {
        "command": "util.base64Encode",
        "title": "%tools.util.base64Encode%"
      },
      {
        "command": "util.base64Decode",
        "title": "%tools.util.base64Decode%"
      },
      {
        "command": "url.escape",
        "title": "%tools.url.escape%"
      },
      {
        "command": "url.unescape",
        "title": "%tools.url.unescape%"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.1",
    "@vscode/vsce": "^2.16.0",
    "eslint": "^8.30.0",
    "glob": "^8.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dayjs": "^1.11.7"
  }
}
